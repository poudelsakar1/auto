    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.153",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.0",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-slick": "^0.23.4",
    "@types/reactstrap": "^8.4.2",
    "@types/styled-components": "^5.1.7",
    "@types/yup": "^0.29.7",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "classnames": "^2.3.1",
    "formik": "^2.1.5",
    "fs": "0.0.1-security",
    "global": "^4.4.0",
    "lodash": "^4.17.15",
    "prettier": "^2.3.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-recaptcha": "^2.1.0",
    "react-google-recaptcha-v3": "^1.8.0",
    "react-helmet": "^6.1.0",
    "react-input-range": "^1.3.0",
    "react-notifications-component": "^3.0.3",
    "react-player": "^2.7.2",
    "react-responsive-carousel": "^3.2.9",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-share": "^4.4.0",
    "react-simple-tooltip": "^2.6.1",
    "react-slick": "^0.26.1",
    "reactjs-popup": "^2.0.4",
    "reactstrap": "^8.4.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.2.1",
    "tailwindcss": "^1.4.6",
    "typescript": "~3.7.2",
    "yup": "^0.29.3"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "bundle": "webpack",
    "clean": "shx rm -rf ./dist",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --hot",
    "format": "eslint --ignore-path=.gitignore --ext .ts,.tsx --fix . && prettier --ignore-path=.gitignore --loglevel=error --write .",
    "check-eslint": "eslint --ignore-path=.gitignore --ext .ts,.tsx --max-warnings=0 .",
    "check-prettier": "prettier --ignore-path=.gitignore --list-different .",
    "check-typescript": "tsc",
    "check-all": "yarn check-prettier && yarn check-eslint && yarn check-typescript",
    "dev": "npm-run-all --parallel start-dev-server start",
    "start-dev-server": "node ../autolease-ninja-middleware/app.js",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --mode production --open --hot",
    "widget": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --hot --entryDev=widget",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production",
    "invalidation-dev": "aws cloudfront create-invalidation --distribution-id E2RBIEG6H44NOM --path /*",
    "upload-dev": "aws s3 sync ./dist s3://bootsketch --cache-control max-age=604800",
    "deploy-dev": "npm run build && npm run upload-dev && npm run invalidation-dev",
    "upload": "aws s3 sync ./dist s3://autoleaseninjas.net --cache-control max-age=2",
    "devupload": "aws s3 sync ./dist s3://autolease-temp --cache-control max-age=2",
    "invalidation": "aws cloudfront create-invalidation --distribution-id E2YK1PKW8BAXGZ --path '/*' --profile autolease",
    "deploy": "npm run build && npm run upload && npm run invalidation"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-knobs": "^6.0.21",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/react-notifications-component": "^2.4.0",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-loader": "^3.0.0",
    "shx": "^0.3.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
